when(Testing_lists, {
    should(init_lists_with_zero_size, {
        List *l = List_new();

        assert_equal(0, l->size, "New list size must be 0.");
    })

    should(append_to_lists, {
        List *l = List_new();
        List_append(l, "test");

        assert_equal(1, l->size, "List after append should have size of 1.");
    })

    should(pop_from_lists, {
        List *l = List_new();
        char *str = "test";

        List_append(l, str);
        assert_equal(str, List_pop(l), "Pointer of inserted element must match original pointer.");
    })
})